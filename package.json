{
  "name": "rescale-design-system",
  "version": "1.0.0",
  "description": "Rescale Design System - A comprehensive component library built on Ant Design for Rescale applications",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "keywords": [
    "react",
    "design-system",
    "component-library",
    "antd",
    "rescale",
    "typescript",
    "styled-components"
  ],
  "author": "Rescale Design System Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobsurber/rescale-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/jacobsurber/rescale-design-system/issues"
  },
  "homepage": "https://jacobsurber.github.io/rescale-design-system",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:storybook": "storybook build",
    "build:all": "npm run build:lib && npm run build:storybook",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "preview:all": "npm run build:all && vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:accessibility": "jest --testNamePattern=\"Accessibility\" --watchAll=false",
    "test:ci": "jest --coverage --watchAll=false --ci --reporters=default --reporters=jest-junit",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "prepublishOnly": "npm run build:lib && npm run test:ci && npm run lint",
    "release": "npm run build:lib && npm publish"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.0 || ^6.0.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/styled-components": "^5.1.34",
    "antd": "^5.0.0",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "styled-components": "^6.0.0",
    "zustand": "^5.0.6"
  },
  "peerDependencies": {
    "@ant-design/icons": "^5.0.0 || ^6.0.0",
    "antd": "^5.0.0",
    "dayjs": "^1.11.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@storybook/addon-actions": "^8.4.6",
    "@storybook/addon-docs": "^8.4.6",
    "@storybook/addon-onboarding": "^8.4.6",
    "@storybook/react-vite": "^8.4.6",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.6.0",
    "chromatic": "^11.15.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^0.11.0",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^30.0.4",
    "prettier": "^3.6.2",
    "storybook": "^8.4.6",
    "styled-components": "^6.1.19",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2"
  }
}
