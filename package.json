{
  "name": "rescale-design-system",
  "version": "1.0.0",
  "description": "Rescale Design System - A comprehensive component library built on Ant Design for Rescale applications",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "keywords": [
    "react",
    "design-system",
    "component-library",
    "antd",
    "rescale",
    "typescript",
    "styled-components"
  ],
  "author": "Rescale Design System Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rescale/design-system.git"
  },
  "bugs": {
    "url": "https://github.com/rescale/design-system/issues"
  },
  "homepage": "https://rescale.github.io/design-system",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:storybook": "storybook build",
    "build:all": "npm run build:lib && npm run build:storybook",
    "copy:storybook": "cp -r storybook-static dist/storybook",
    "deploy": "npm run build:lib && gh-pages -d dist",
    "deploy:all": "npm run build:all && gh-pages -d dist",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "preview:all": "npm run build:all && vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:accessibility": "jest --testNamePattern=\"Accessibility\" --watchAll=false",
    "test:ci": "jest --coverage --watchAll=false --ci --reporters=default --reporters=jest-junit",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "prepare": "husky install",
    "prepublishOnly": "npm run build:lib && npm run test:ci && npm run lint",
    "release": "npm run build:lib && npm publish"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@types/styled-components": "^5.1.34",
    "antd": "^5.26.6",
    "dayjs": "^1.11.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "styled-components": "^6.1.19"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "antd": "^5.0.0",
    "@ant-design/icons": "^5.0.0 || ^6.0.0",
    "styled-components": "^6.0.0",
    "dayjs": "^1.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@eslint/js": "^9.30.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@storybook/addon-a11y": "^9.0.17",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-backgrounds": "^9.0.17",
    "@storybook/addon-controls": "^9.0.17",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-measure": "^9.0.17",
    "@storybook/addon-onboarding": "^9.0.17",
    "@storybook/addon-outline": "^9.0.17",
    "@storybook/addon-viewport": "^9.0.17",
    "@storybook/manager-api": "^9.0.17",
    "@storybook/react-vite": "^9.0.17",
    "@storybook/theming": "^9.0.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.6.0",
    "antd": "^5.26.6",
    "axe-core": "^4.10.2",
    "chromatic": "^11.15.0",
    "dayjs": "^1.11.13",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.0.17",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^9.0.17",
    "styled-components": "^6.1.19",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-lib-inject-css": "^2.1.1"
  }
}
